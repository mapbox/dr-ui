{
  "name": "@mapbox/dr-ui",
  "version": "0.8.0",
  "description": "Mapbox frontend tools for documentation websites.",
  "main": "index.js",
  "scripts": {
    "start": "budo ./src/test-cases-app/test-cases-app.js --dir ./src/test-cases-app --live --debug --pushstate -- -t babelify",
    "cp-assembly": "del ./src/test-cases-app/dist && cpy node_modules/@mapbox/mapbox-assembly/dist/*.* ./src/test-cases-app/dist && cpy src/css/docs-prose.css ./src/test-cases-app/dist && cpy src/css/prism.css ./src/test-cases-app/dist",
    "budo": "budo test/app.js -l -d -P -- -t babelify",
    "prestart": "npm run build-component-index && npm run cp-assembly",
    "test": "jest tests/*.test.js",
    "build-component-index": "rtk-index ./src/components ./src/test-cases-app/component-index.js",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "precommit": "lint-staged",
    "format": "prettier '**/*.js' --write",
    "build": "scripts/build-module-indexes.js && scripts/build-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/frontend-documentation.git"
  },
  "author": "Mapbox",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/mapbox/frontend-documentation/issues"
  },
  "homepage": "https://github.com/mapbox/frontend-documentation#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "@mapbox/eslint-config-mapbox": "^1.2.1",
    "@mapbox/mapbox-assembly": "^0.26.0",
    "@mapbox/react-test-kitchen": "^0.1.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babelify": "^10.0.0",
    "budo": "^11.6.0",
    "cpy": "^7.0.1",
    "cpy-cli": "^2.0.0",
    "del": "^3.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.12.4",
    "execa": "^0.10.0",
    "hard-rejection": "^1.0.0",
    "highlight.js": "^9.13.1",
    "husky": "^0.14.3",
    "jest": "^24.1.0",
    "lint-staged": "^7.3.0",
    "make-dir": "^1.3.0",
    "prettier": "^1.16.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "unist-util-visit": "^1.4.0"
  },
  "dependencies": {
    "@mapbox/mr-ui": "^0.5.0",
    "classnames": "^2.2.6",
    "debounce": "^1.2.0",
    "extend": "^3.0.2",
    "hastscript": "^5.0.0",
    "react-stickynode": "^2.1.1",
    "rehype-sectionize-headings": "^1.0.0-rc.1"
  }
}
